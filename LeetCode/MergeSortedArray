class Solution {
    func merge(_ nums1: inout [Int], _ m: Int, _ nums2: [Int], _ n: Int) {
        var tmp1 = [Int]()
        
        for index in 0..<m {
            tmp1.append(nums1[index])
        }
        
        for index in 0..<n {
            tmp1.append(nums2[index])
        }
        
        tmp1.sort()
        nums1 = tmp1
    }
}
